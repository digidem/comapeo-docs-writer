{
  "name": "comapeo-docs-writer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:md": "markdownlint \"**/*.md\" --ignore node_modules --ignore dist",
    "docs:build": "node scripts/build-docs.js",
    "docs:serve": "node scripts/serve-docs.js",
    "docs": "npm run docs:build && npm run docs:serve",
    "docs:open": "xdg-open http://localhost:4000 || open http://localhost:4000 || start http://localhost:4000",
    "update-prompts": "mkdir -p \"$HOME/.codex/prompts\" && cp -a context/prompts/. \"$HOME/.codex/prompts/\"",
    "caption:all": "prompt_content=\"$(cat context/prompts/caption-all-images.md)\" codex --dangerously-bypass-approvals-and-sandbox --full-auto exec \"$prompt_content\"",
    "caption": "prompt_content=\"$(cat context/prompts/caption-image.md)\" codex --dangerously-bypass-approvals-and-sandbox --full-auto exec \"$prompt_content\"",
    "gen:all": "node scripts/generate_missing_versions.js",
    "gen:test": "node scripts/generate_missing_versions.js --limit 3",
    "check:links": "node scripts/check_links.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@openai/codex": "^0.34.0"
  },
  "devDependencies": {
    "markdownlint-cli": "^0.45.0"
  }
}
