{
  "name": "comapeo-docs-writer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:md": "markdownlint \"**/*.md\" --ignore node_modules --ignore dist",
    "docs:build": "node scripts/build-docs.js",
    "docs:serve": "node scripts/serve-docs.js",
    "docs": "npm run docs:build && npm run docs:serve",
    "docs:open": "xdg-open http://localhost:4000 || open http://localhost:4000 || start http://localhost:4000",
    "update-prompts": "mkdir -p \"$HOME/.codex/prompts\" && cp -a context/prompts/. \"$HOME/.codex/prompts/\"",
    "caption:all": "codex -m gpt-5 --dangerously-bypass-approvals-and-sandbox exec \"$(cat context/prompts/caption-all-images.md)\"",
    "caption": "codex -m gpt-5 --dangerously-bypass-approvals-and-sandbox exec \"$(cat context/prompts/caption-image.md)\"",
    "gen": "node scripts/gen.js",
    "gen:all": "node scripts/gen-all.js",
    "gen:smoke": "node scripts/gen-smoke.js",
    "gen:test": "node scripts/gen-test.js",
    "test:ds": "node scripts/gen-test-ds.js",
    "gen:next": "node scripts/gen.js",
    "check:links": "node scripts/check_links.js",
    "audit": "node scripts/smart_audit.js",
    "show-prompt": "node scripts/show-prompt.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@openai/codex": "^0.63.0"
  },
  "devDependencies": {
    "markdownlint-cli": "^0.46.0"
  }
}
